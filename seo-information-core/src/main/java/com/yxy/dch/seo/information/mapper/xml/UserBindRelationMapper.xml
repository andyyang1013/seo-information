<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.yxy.dch.seo.information.mapper.UserBindRelationMapper">

    <resultMap id="customResultMap" type="com.yxy.dch.seo.information.entity.UserBindRelation">
        <result property="id" column="id"/>
        <result property="userId" column="user_id"/>
        <result property="bindUserIds" column="bind_user_ids"/>
        <result property="delFlag" column="del_flag"/>
        <result property="createTime" column="create_time"/>
        <result property="createUid" column="create_uid"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateUid" column="update_uid"/>
    </resultMap>

    <sql id="columns">
        	id,
        	user_id,
        	bind_user_ids,
        	del_flag,
        	create_time,
        	create_uid,
        	update_time,
        	update_uid
	</sql>

    <select id="selectByUserId" parameterType="long" resultMap="customResultMap">
        select
        <include refid="columns"/>
        from t_user_bind_relation where user_id = #{curUserId}
    </select>


</mapper>

